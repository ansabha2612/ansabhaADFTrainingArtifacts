{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ansabhaADFTraining"
		},
		"BlobMetadataSQLConnection_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'BlobMetadataSQLConnection'"
		},
		"OrdersBlobStorageConnection_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'OrdersBlobStorageConnection'"
		},
		"OrganizedDLSConnection_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'OrganizedDLSConnection'"
		},
		"OrdersDataWareHouse_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'OrdersDataWareHouse'"
		},
		"DLSTargetProviderFunction_properties_typeProperties_functionAppUrl": {
			"type": "string",
			"defaultValue": "https://dlstargetprovider20201125160234.azurewebsites.net"
		},
		"OrganizedDLSConnection_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://ansabhatrainingadlstrgt.dfs.core.windows.net"
		},
		"TrainingKVConnection_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://ansabhakeyvaulttraining.vault.azure.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ansabhaADFTraining_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"OrdersBlobStorageConnection_connectionString": {
						"value": "[parameters('OrdersBlobStorageConnection_connectionString')]"
					},
					"OrganizedDLSConnection_accountKey": {
						"value": "[parameters('OrganizedDLSConnection_accountKey')]"
					},
					"OrdersDataWareHouse_connectionString": {
						"value": "[parameters('OrdersDataWareHouse_connectionString')]"
					},
					"BlobMetadataSQLConnection_connectionString": {
						"value": "[parameters('BlobMetadataSQLConnection_connectionString')]"
					},
					"OrganizedDLSConnection_properties_typeProperties_url": {
						"value": "[parameters('OrganizedDLSConnection_properties_typeProperties_url')]"
					},
					"TrainingKVConnection_properties_typeProperties_baseUrl": {
						"value": "[parameters('TrainingKVConnection_properties_typeProperties_baseUrl')]"
					},
					"DLSTargetProviderFunction_properties_typeProperties_functionAppUrl": {
						"value": "[parameters('DLSTargetProviderFunction_properties_typeProperties_functionAppUrl')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}